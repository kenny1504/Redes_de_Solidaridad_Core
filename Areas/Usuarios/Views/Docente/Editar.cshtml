
<!--form para poder activar la ruta y poder guardar el registro--><!-- -->
<form id="editar_Docentes2" onsubmit="event.preventDefault()">
	<div class="modal  modal-info fade" id="editar_Docente2">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
					<h4 class="modal-title">Editando Usuario Docente</h4>
				</div>
				<div class="modal-body ">
					<div class="register-box-body" style="background-color:#eee;">
						<div class="form-group has-feedback">
							<input type="text" id="Id_UsuarioD" name="Id" class="form-control requerido hidden" required>
							<input type="text" id="CedulaU2" autocomplete="off" name="Cedula" class="form-control requerido" required placeholder="Cedula de docente">
							<small>Cedula de usuario</small>
						</div>
						<div class="form-group has-feedback">
							<input type="text" id="UsuarioU2" name="Usuario" class="form-control requerido" required placeholder="Nombre de Usuario">
							<small>Nombre de usuario</small>
						</div>
						<div class="form-group has-feedback">
							<input type="password" id="ContrasenaU2" name="Contraseña" class="form-control requerido" autocomplete="new-password" required placeholder="Contraseña">
							<small>Contraseña de usuario</small>
						</div>
						<div class="form-group has-feedback">
							<input  id="Instituciones2" name="Institucion" class="form-control requerido hidden" autocomplete="new-password">
						</div>
					</div>
					<div class="modal-footer">
						<button type="reset" class="btn btn-outline pull-left" data-dismiss="modal">Cancelar</button>
						<button onclick="editar_Usuario_Docente()" class="btn btn-outline">Guardar</button>
					</div>
					<!-- /.modal-content -->
				</div>
				<!-- /.modal-dialog -->
			</div>
		</div>
	</div>
</form>
