@{
	ViewData["Title"] = "Agregar";
}

<!--form para poder activar la ruta y poder guardar el registro--><!-- -->
<form id="ingresar_Docentes2" onsubmit="event.preventDefault()">
	<div class="modal  modal-info fade" id="ingresar_Docente2">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
					<h4 class="modal-title">Ingresar Usuario Docente</h4>
				</div>
				<div class="modal-body ">
					<div class="register-box-body" style="background-color:#eee;">
						<div class="form-group has-feedback">
							<input type="text" id="CedulaU" autocomplete="off" name="Cedula" class="form-control requerido" required placeholder="Cedula de docente">
						</div>
						<div class="form-group has-feedback">
							<input type="text" id="UsuarioU" name="Usuario" class="form-control requerido" required placeholder="Nombre de Usuario">
						</div>
						<div class="form-group has-feedback">
							<input type="password" id="ContrasenaU" name="Contraseña" class="form-control requerido" autocomplete="new-password" required placeholder="Contraseña">
						</div>
						<div class="form-group has-feedback">
							<input  id="Instituciones" name="Institucion" class="form-control requerido hidden">
						</div>
					</div>
					<div class="modal-footer">
						<button type="reset" class="btn btn-outline pull-left" data-dismiss="modal">Cancelar</button>
						<button type="submit" onclick="ingresar_usuario_docente()" class="btn btn-outline">Guardar</button>
					</div>
					<!-- /.modal-content -->
				</div>
				<!-- /.modal-dialog -->
			</div>
		</div>
	</div>
</form>